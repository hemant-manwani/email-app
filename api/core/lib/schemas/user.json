{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
	"_id": {
	  "type": "object",
	  "description": "Unique identifier of User"
	},
	"firstName": {
	  "type": "string"
	},
	"lastName": {
	  "type": "string"
	},
	"email": {
	  "type": "string",
	  "description": "Primary email address for communication",
	  "format": "email"
	},
	"avatar": {
	  "type": "string",
	  "description": "User's profile picture",
	  "format": "uri"
	},
	"technology": {
	  "type": "string",
	  "description": "User's technology"
	},
	"created": {
	  "description": "The time at which this user was created.",
	  "type": "object"
	},
	"follow_up_date": {
	  "description": "The time at which this user was followed up.",
	  "type": "object"
	}
	}
}
